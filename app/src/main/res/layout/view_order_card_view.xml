<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/txtViewOrderToyQty"
        android:layout_width="66dp"
        android:layout_height="36dp"
        android:layout_marginTop="16dp"
        android:text="10"
        android:textAlignment="center"
        android:textColor="#FE6D40"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@+id/txtViewOrderToyName"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtViewOrderToyPrice"
        android:layout_width="131dp"
        android:layout_height="35dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="16dp"
        android:text="14.99"
        android:textAlignment="textEnd"
        android:textColor="#707070"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/txtViewOrderToyQty"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtViewOrderToyName"
        android:layout_width="166dp"
        android:layout_height="36dp"
        android:layout_marginTop="16dp"
        android:text="Dinasaur"
        android:textAlignment="viewStart"
        android:textColor="#FE6D40"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.065"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>